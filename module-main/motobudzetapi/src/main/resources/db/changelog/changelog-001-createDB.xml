    <changeSet id="create_city_states_table" author="michal-k">
        <createTable tableName="city_states">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>


    <changeSet id="test" author="michal-k">
        <createTable tableName="cities">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="e_longitude" type="double precision">
                <constraints nullable="false"/>
            </column>
            <column name="n_latitude" type="double precision">
                <constraints nullable="false"/>
            </column>
            <column name="state_id" type="bigint">
                <constraints nullable="true" foreignKeyName="fk8akwkumky4alc0edjtn1ypr1o" referencedTableName="city_states" referencedColumnNames="id"/>
            </column>
            <column name="name" type="varchar(150)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="1" author="michal-k">
        <createTable tableName="brand">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false" unique="true"/> <!-- Dodana opcja unique -->
            </column>
        </createTable>
    </changeSet>

    <changeSet id="2" author="michal-k">
        <createTable tableName="drive_type">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="3" author="michal-k">
        <createTable tableName="engine_type">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="4" author="michal-k">
        <createTable tableName="fuel_type">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="5" author="michal-k">
        <createTable tableName="model">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="brand_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="6" author="michal-k">
        <createTable tableName="brand_model_list">
            <column name="brand_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="model_list_id" type="bigint">
                <constraints nullable="false" unique="true"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="7" author="michal-k">
        <createTable tableName="transmission_type">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="8" author="michal-k">
        <createTable tableName="advertisement">
            <column name="id" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="is_verified" type="boolean" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
            <column name="is_active" type="boolean" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
            <column name="is_deleted" type="boolean" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
            <column name="brand_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="city_id" type="bigint"/>
            <column name="model_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="main_photo_url" type="varchar(255)"/>

            <column name="create_date" type="timestamp">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified" type="timestamp"/>
            <column name="last_modified_by" type="bigint"/>
            <column name="engine_capacity" type="bigint"/>
            <column name="engine_horse_power" type="bigint"/>
            <column name="first_registration_date" type="date"/>
            <column name="mileage" type="bigint"/>
            <column name="mileage_unit" type="varchar(255)">
                <constraints/>
            </column>
            <column name="price" type="bigint"/>
            <column name="price_unit" type="varchar(255)">
                <constraints/>
            </column>
            <column name="production_date" type="bigint"/>
            <column name="description" type="varchar(60000)"/>
            <column name="drive_type_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="engine_type_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="fuel_type_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="transmission_type_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="brand_id"
                                 referencedTableName="brand"
                                 referencedColumnNames="id"
                                 constraintName="fksmrjb6imcn6lfd82wh045ntu5"/>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="city_id"
                                 referencedTableName="cities"
                                 referencedColumnNames="id"
                                 constraintName="fkl1p9vos2flnbm4bepw7y7u8on"/>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="drive_type_id"
                                 referencedTableName="drive_type"
                                 referencedColumnNames="id"
                                 constraintName="fknj8gdmre6khpjsoy6kdkbj7ai"/>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="engine_type_id"
                                 referencedTableName="engine_type"
                                 referencedColumnNames="id"
                                 constraintName="fklxhcfot1h4t04dec4e4175haj"/>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="fuel_type_id"
                                 referencedTableName="fuel_type"
                                 referencedColumnNames="id"
                                 constraintName="fk1c97ccvnaal9aw8v5viwdlw5v"/>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="model_id"
                                 referencedTableName="model"
                                 referencedColumnNames="id"
                                 constraintName="fk4cbs20m6gc5aueipii0cu0nfc"/>

        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="transmission_type_id"
                                 referencedTableName="transmission_type"
                                 referencedColumnNames="id"
                                 constraintName="fkc2bcd874c4023wdmehxjqgvps"/>
    </changeSet>

    <changeSet id="9" author="michal-k">
        <createTable tableName="advertisement_images">
            <column name="advertisement_id" type="uuid">
                <constraints nullable="false"/>
            </column>
            <column name="image_urls" type="varchar(255)"/>
        </createTable>
    </changeSet>

    <changeSet id="10" author="michal-k">
        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="brand_id"
                                 constraintName="fk_brand_advertisement"
                                 referencedTableName="brand"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"/>
        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="drive_type_id"
                                 constraintName="fk_drive_type_advertisement"
                                 referencedTableName="drive_type"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"/>
        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="engine_type_id"
                                 constraintName="fk_engine_type_advertisement"
                                 referencedTableName="engine_type"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"/>
        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="fuel_type_id"
                                 constraintName="fk_fuel_type_advertisement"
                                 referencedTableName="fuel_type"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"/>
        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="model_id"
                                 constraintName="fk_model_advertisement"
                                 referencedTableName="model"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"/>
        <addForeignKeyConstraint baseTableName="advertisement"
                                 baseColumnNames="transmission_type_id"
                                 constraintName="fk_transmission_type_advertisement"
                                 referencedTableName="transmission_type"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"/>
    </changeSet>


</databaseChangeLog>
