<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="insert_brands" author="michal-k">
        <loadData tableName="brand" file="classpath:/db/changelog/csv/brand.csv" separator=",">
            <column name="id" type="INT"/>
            <column name="name" type="STRING"/>
        </loadData>
    </changeSet>


    <changeSet id="insert_models" author="michal-k">
        <loadData tableName="model" file="classpath:/db/changelog/csv/model.csv" separator=",">
            <column name="id" type="INT"/>
            <column name="name" type="STRING"/>
            <column name="brand_id" type="INT"/>
        </loadData>
    </changeSet>

    <changeSet id="insert_brand_model_relation" author="michal-k">
        <loadData tableName="brand_model_list" file="classpath:/db/changelog/csv/brand_model_list.csv" separator=",">
            <column name="brand_id" type="INT"/>
            <column name="model_list_id" type="INT"/>
        </loadData>
    </changeSet>

</databaseChangeLog>
